{
  "name": "vite-static-assets-plugin",
  "type": "module",
  "version": "1.0.5",
  "description": "Typesafe static assets with Vite",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo 'No tests yet' && exit 0",
    "prepare": "npm run typecheck && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinBspheroid/vite-static-assets-plugin.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "static"
  ],
  "author": "Martin Blasko",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.1",
    "typescript": "^5.7.0",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "minimatch": "^10.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "vite": "^6.1.0"
  }
}